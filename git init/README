// PT.1 WORKS
var node_mainnavs = document.getElementsByClassName("nav mainnavs")[0];
var node_parent_mainnavs = node_mainnavs.parentNode;
node_parent_mainnavs.removeChild(node_mainnavs);

// PT.2 WORKS
var my_sidebar = document.getElementById("sidebar");
var my_sidebar_length = my_sidebar.children.length;
for (var i = 0; i < my_sidebar_length; ++i) {
	if (i != 3) {
		if (i < 3) {
			my_sidebar.removeChild(my_sidebar.children[0]);
		}
		else {
			my_sidebar.removeChild(my_sidebar.children[1]);
		}
	}
}

// PT.3 WORKS
var comments_bar = document.getElementsByClassName("comments");
var comments_bar_length = comments_bar.length;
for (var i = 0; i < comments_bar_length; ++i) {
	comments_bar[0].parentNode.removeChild(comments_bar[0]);
}

// PT.4 WORKS
var bottom_bar = document.getElementsByClassName("fw");
var bottom_bar_length = bottom_bar.length;
for (var i = 0; i < bottom_bar_length; ++i) {
	bottom_bar[0].parentNode.removeChild(bottom_bar[0]);



}

// PT.5 WORKS
var comments_link = document.getElementsByClassName("comments-link");
var comments_link_length = comments_link.length;
for (var i = 0; i < comments_link_length; ++i) {
	comments_link[0].parentNode.removeChild(comments_link[0]);



}



var expander = document.getElementsByClassName("post-text");
var expander_length = expander.length;
for (var i = 0; i < expander_length; ++i) {
		expander[i].style.width = "100%";
	}





